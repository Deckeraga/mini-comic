{
  "name": "mini-comic",
  "version": "1.0.0",
  "description": "A barebones comic book reader in electron.",
  "main": "main.js",
  "author": "Alec Decker",
  "license": "MIT",
  "scripts": {
    "package": "npm run build && electron-packager . --overwrite --icon=images/icon.icns",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "start": "npm run build && electron ."
  },
  "keywords": [],
  "devDependencies": {
    "electron": "^4.1.4",
    "electron-packager": "^13.1.1",
    "prettier": "^1.17.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "electron-unrar-js": "^1.0.1",
    "rimraf": "^2.6.3",
    "temp": "^0.9.0",
    "unzipper": "^0.9.11"
  }
}
